{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "prefer-promise-reject-errors": "off",
        "no-use-before-define": "off"
    }
}
